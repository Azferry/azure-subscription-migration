# EditorConfig is awesome: https://EditorConfig.org

root = true


[*]
end_of_line = lf
insert_final_newline = true
indent_size = 4

[*.py]
indent_style = space
indent_size = 4

[*.tf]
indent_style = space
indent_size = 2
[*.tfvars]
indent_style = space
indent_size = 2

[Makefile]
indent_style = tab
